{
  "name": "dsagent",
  "displayName": "DSAgent - AI Data Science Assistant",
  "description": "AI-powered data science agent for VS Code. Analyze data, create visualizations, and build models with natural language.",
  "version": "0.1.1",
  "publisher": "nmlemus",
  "repository": {
    "type": "git",
    "url": "https://github.com/nmlemus/dsagent-vscode"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Data Science",
    "Machine Learning",
    "Notebooks",
    "Chat"
  ],
  "keywords": [
    "data science",
    "ai",
    "machine learning",
    "jupyter",
    "pandas",
    "python"
  ],
  "activationEvents": [
    "onChatParticipant:dsagent.chat",
    "onCommand:dsagent.openChat",
    "onCommand:dsagent.startChat",
    "onView:dsagent.sessionsView",
    "onView:dsagent.variablesView",
    "onView:dsagent.artifactsView",
    "onLanguage:python",
    "onNotebook:jupyter-notebook"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "chatParticipants": [
      {
        "id": "dsagent.chat",
        "fullName": "DSAgent",
        "name": "dsagent",
        "description": "AI-powered data science assistant. Analyze data, create visualizations, and build models.",
        "isSticky": true,
        "commands": [
          {
            "name": "analyze",
            "description": "Analyze a dataset or code"
          },
          {
            "name": "visualize",
            "description": "Create a visualization"
          },
          {
            "name": "model",
            "description": "Build a machine learning model"
          }
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dsagent",
          "title": "DSAgent",
          "icon": "resources/icons/dsagent.svg"
        }
      ]
    },
    "views": {
      "dsagent": [
        {
          "id": "dsagent.sessionsView",
          "name": "Sessions"
        },
        {
          "id": "dsagent.variablesView",
          "name": "Variables"
        },
        {
          "id": "dsagent.artifactsView",
          "name": "Artifacts"
        }
      ]
    },
    "commands": [
      {
        "command": "dsagent.openChat",
        "title": "Open Chat",
        "category": "DSAgent",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "dsagent.startChat",
        "title": "Start New Chat",
        "category": "DSAgent",
        "icon": "$(add)"
      },
      {
        "command": "dsagent.configureServer",
        "title": "Configure Server Connection",
        "category": "DSAgent",
        "icon": "$(gear)"
      },
      {
        "command": "dsagent.connectServer",
        "title": "Connect to Server",
        "category": "DSAgent",
        "icon": "$(plug)"
      },
      {
        "command": "dsagent.disconnectServer",
        "title": "Disconnect from Server",
        "category": "DSAgent",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "dsagent.analyzeSelection",
        "title": "Analyze with DSAgent",
        "category": "DSAgent",
        "icon": "$(sparkle)"
      },
      {
        "command": "dsagent.analyzeFile",
        "title": "Analyze File with DSAgent",
        "category": "DSAgent",
        "icon": "$(sparkle)"
      },
      {
        "command": "dsagent.refreshSessions",
        "title": "Refresh Sessions",
        "category": "DSAgent",
        "icon": "$(refresh)"
      },
      {
        "command": "dsagent.openNotebook",
        "title": "Open Session Notebook",
        "category": "DSAgent",
        "icon": "$(notebook)"
      },
      {
        "command": "dsagent.refreshNotebook",
        "title": "Refresh Session Notebook",
        "category": "DSAgent",
        "icon": "$(sync)"
      },
      {
        "command": "dsagent.exportNotebook",
        "title": "Export Session Notebook",
        "category": "DSAgent",
        "icon": "$(cloud-download)"
      },
      {
        "command": "dsagent.refreshArtifacts",
        "title": "Refresh Artifacts",
        "category": "DSAgent",
        "icon": "$(refresh)"
      },
      {
        "command": "dsagent.openArtifact",
        "title": "Open Artifact",
        "category": "DSAgent",
        "icon": "$(open-preview)"
      },
      {
        "command": "dsagent.previewDataFrame",
        "title": "Preview DataFrame",
        "category": "DSAgent",
        "icon": "$(open-preview)"
      },
      {
        "command": "dsagent.inspectVariable",
        "title": "Inspect Variable",
        "category": "DSAgent",
        "icon": "$(eye)"
      },
      {
        "command": "dsagent.copyVariableName",
        "title": "Copy Name",
        "category": "DSAgent",
        "icon": "$(clippy)"
      },
      {
        "command": "dsagent.describeDataFrame",
        "title": "Describe",
        "category": "DSAgent",
        "icon": "$(graph)"
      },
      {
        "command": "dsagent.dataFrameInfo",
        "title": "Info",
        "category": "DSAgent",
        "icon": "$(info)"
      },
      {
        "command": "dsagent.plotVariable",
        "title": "Quick Plot",
        "category": "DSAgent",
        "icon": "$(graph-line)"
      },
      {
        "command": "dsagent.analyzeVariable",
        "title": "Analyze with DSAgent",
        "category": "DSAgent",
        "icon": "$(sparkle)"
      },
      {
        "command": "dsagent.approveAction",
        "title": "Approve Action",
        "category": "DSAgent"
      },
      {
        "command": "dsagent.rejectAction",
        "title": "Reject Action",
        "category": "DSAgent"
      },
      {
        "command": "dsagent.setHitlMode",
        "title": "Set Human-in-the-Loop Mode",
        "category": "DSAgent",
        "icon": "$(shield)"
      },
      {
        "command": "dsagent.refreshVariables",
        "title": "Refresh Variables",
        "category": "DSAgent",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "dsagent.analyzeSelection",
          "when": "editorHasSelection && editorLangId == python",
          "group": "dsagent@1"
        }
      ],
      "explorer/context": [
        {
          "command": "dsagent.analyzeFile",
          "when": "resourceExtname == .csv || resourceExtname == .xlsx || resourceExtname == .json || resourceExtname == .parquet",
          "group": "dsagent@1"
        }
      ],
      "view/item/context": [
        {
          "command": "dsagent.previewDataFrame",
          "when": "viewItem == dataframe",
          "group": "inline"
        },
        {
          "command": "dsagent.copyVariableName",
          "when": "viewItem == dataframe || viewItem == variable",
          "group": "1_actions@0"
        },
        {
          "command": "dsagent.describeDataFrame",
          "when": "viewItem == dataframe",
          "group": "1_actions@1"
        },
        {
          "command": "dsagent.dataFrameInfo",
          "when": "viewItem == dataframe",
          "group": "1_actions@2"
        },
        {
          "command": "dsagent.plotVariable",
          "when": "viewItem == dataframe",
          "group": "2_visualize@0"
        },
        {
          "command": "dsagent.analyzeVariable",
          "when": "viewItem == dataframe || viewItem == variable",
          "group": "3_dsagent@0"
        },
        {
          "command": "dsagent.inspectVariable",
          "when": "viewItem == variable",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "dsagent.startChat",
          "when": "view == dsagent.sessionsView",
          "group": "navigation@0"
        },
        {
          "command": "dsagent.refreshSessions",
          "when": "view == dsagent.sessionsView",
          "group": "navigation@1"
        },
        {
          "command": "dsagent.refreshVariables",
          "when": "view == dsagent.variablesView",
          "group": "navigation@1"
        },
        {
          "command": "dsagent.exportNotebook",
          "when": "view == dsagent.artifactsView",
          "group": "navigation@0"
        },
        {
          "command": "dsagent.refreshArtifacts",
          "when": "view == dsagent.artifactsView",
          "group": "navigation@1"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "dsagent.sessionsView",
        "contents": "No sessions yet.\n[Start New Chat](command:dsagent.startChat)"
      },
      {
        "view": "dsagent.artifactsView",
        "contents": "No artifacts yet. Artifacts will appear here as the agent generates outputs."
      }
    ],
    "configuration": {
      "title": "DSAgent",
      "properties": {
        "dsagent.serverUrl": {
          "type": "string",
          "default": "http://localhost:8000",
          "description": "DSAgent server URL (e.g. http://localhost:8000 or https://remote-server.example.com:8000)",
          "order": 1
        },
        "dsagent.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for authenticating with a remote DSAgent server (matches DSAGENT_API_KEY on the server)",
          "order": 2
        },
        "dsagent.autoConnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically connect to server on startup",
          "order": 3
        },
        "dsagent.model": {
          "type": "string",
          "default": "gpt-4o",
          "enum": [
            "gpt-4o",
            "gpt-4o-mini",
            "claude-sonnet-4-20250514",
            "claude-opus-4-20250514",
            "gemini-2.0-flash",
            "gemini-2.5-pro"
          ],
          "description": "Default LLM model to use"
        },
        "dsagent.hitlMode": {
          "type": "string",
          "default": "none",
          "enum": [
            "none",
            "plan_only",
            "full",
            "plan_and_answer",
            "on_error"
          ],
          "enumDescriptions": [
            "No human approval required â€” agent runs autonomously",
            "Require approval before executing a plan",
            "Require approval for every plan and code execution",
            "Require approval for plans and final answers",
            "Require approval only when errors occur"
          ],
          "description": "Human-in-the-Loop mode for new sessions. Controls when the agent pauses for approval.",
          "order": 4
        },
        "dsagent.showThinking": {
          "type": "boolean",
          "default": true,
          "description": "Show thinking indicator while agent is processing",
          "order": 5
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run build:webview && node esbuild.js --production",
    "build:webview": "cd webview-ui && npm run build",
    "watch": "node esbuild.js --watch",
    "watch:webview": "cd webview-ui && npm run dev",
    "dev": "concurrently \"npm run watch\" \"npm run watch:webview\"",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/vsce": "^2.22.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "ws": "^8.14.0"
  }
}
